{
  "name": "bytexs/gd-text-ng",
  "version": "1.1.2",
  "description": "Forked from stil/gd-text. A class drawing multiline and aligned text on pictures. Uses GD extension.",
  "license": "MIT",
  "type": "library",
  "prefer-stable": true,
  "minimum-stability": "stable",
  "config": {
    "platform": {
      "php": "8.1.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require": {
    "php": ">=8.1",
    "ext-gd": "*"
  },
  "require-dev": {
    "rector/rector": "@stable",
    "phpunit/phpunit": "@stable"
  },
  "autoload": {
    "psr-4": {
      "GDTextNg\\": "src/",
      "GDTextNg\\Tests\\": "tests/"
    }
  }
}
